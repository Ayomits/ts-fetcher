{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@ts-fetcher/cache",
  "version": "1.2.0",
  "description": "Cache package for ts-fetch",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "format:fix": "npx prettier --write \"src/**/*.ts\"",
    "format:check": "npx prettier --check \"src/**/*.ts\"",
    "lint:check": "npx eslint src --ext .ts",
    "lint:fix": "npx eslint src --ext .ts --fix",
    "build": "npx tsc --build && npx tsc-alias",
    "pipeline": "pnpm run format:fix && pnpm run lint:fix && npm run build"
  },
  "keywords": [],
  "author": "Ayomits",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@ts-fetcher/types": "workspace:^",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "ioredis": "^5.6.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"
  }
}
